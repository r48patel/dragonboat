<% include ../partials/header.ejs %>

<!-- Content -->
<section>
	<header class="main">
		<h1>Gallery</h1>
	</header>

	<!-- Elements -->
	<div class="row 200%">
		<div class="">
			<!-- Image -->
			<span class="image fit logo"><img src="images/logo.png" alt="" /></span>
			<div class="box alt">
				<div class="row 50% uniform">
					<% gallery_list.forEach(file => {%>
						<div class="6u"><span class="image fit"><img src="images/gallery/<%=file%>" alt="" /></span></div>
					<% }) %>
				</div>
			</div>
		</div>
	</div>
</section>

</div>
</div>
